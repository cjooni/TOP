  <data name="customSqlQuery8.Sql" xml:space="preserve">
    <value>select b.맨홀  관로번호
       ,'No.'
       , concat('No.', lpad(floor(a.누가거리1/20), 5, ' '), ' +', lpad(mod(a.누가거리1, 20), 6, ' '), c.전후단면) 측점       , a.누가거리1
       , a.누가거리2 
       , b.포장종류 
       , c.육상_토사 
       , case a.seq
       		when 0 then 0
       		else round((d.육상_토사 + c.육상_토사)/2 * a.누가거리2 , 2)       		
         end 육상_부피
       ,c.수중_토사
       ,case a.seq
       		when 0 then 0
       		else round((d.수중_토사 + c.수중_토사)/2 * a.누가거리2 , 2)
         end 수중_부피
       ,c.관주위 
       ,case a.seq
       		when 0 then 0
       		else round((d.관주위 + c.관주위)/2 * a.누가거리2 , 2)
         end 관주위부피
       ,c.관상부 
       ,case a.seq
       		when 0 then 0
       		else round((d.관상부 + c.관상부)/2 * a.누가거리2 , 2)
         end 관상부부피
       ,c.모래부설 as 관기초
       ,case a.seq
       		when 0 then 0
       		else round((d.모래부설 + c.모래부설)/2 * a.누가거리2 , 2)
         end 관기초부피
       ,case b.포장종류 
       		when 'xASP' then a.누가거리2 *2 
       		else a.누가거리2 *2 
        end 절단
       ,case b.포장종류 
       		when 'ASP' then c.ASP 
       		when 'CON''C' then c.CONC 
       		when 'A+C' then c.덧씌우기 
       		when '보도' then C.보도블럭 
       		when '투수콘' then C.ASP 
        end 깨기
       ,case b.포장종류 
       		when 'ASP' then round((d.ASP + c.ASP)/2 * a.누가거리2 , 2)
       		when 'CON''C' then round((d.CONC + c.CONC)/2 * a.누가거리2 , 2)
       		when 'A+C' then round((d.덧씌우기 + c.덧씌우기)/2 * a.누가거리2 , 2)
       		when '보도' then round((d.보도블럭 + c.보도블럭)/2 * a.누가거리2 , 2)
       		when '투수콘' then round((d.ASP + c.ASP)/2 * a.누가거리2 , 2)
        end 복구
       ,c.보조기층 
       ,round((d.보조기층 + c.보조기층)/2 * a.누가거리2 , 2) 보조기층부피
       ,case b.포장종류 
       		when '투수콘' then c.동상방지층 
       		else 0
        end 모래
       ,case b.포장종류 
       		when '투수콘' then round((d.동상방지층 + c.동상방지층)/2 * a.누가거리2 , 2)
       		else 0
        end 모래부피 
   from pipe01t00 a, pipe01m00 b, pipe02m00 c, pipe02m00 d
  where a.PROJECT_CD = @P_PROJECT_CD
    and a.project_cd = b.PROJECT_CD 
    and a.LINENAME  = b.LINENAME
    and a.SEQ  = b.SEQ 
    and a.project_cd = c.PROJECT_CD 
    and a.LINENAME  = c.LINENAME 
    and a.SEQ = c.SEQ 
    and a.project_cd = d.PROJECT_CD 
    and a.LINENAME  = d.LINENAME 
    and a.N_SEQ = d.SEQ</value>
