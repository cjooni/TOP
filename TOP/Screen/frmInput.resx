  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>INSERT INTO PIPE01M00 (PROJECT_CD
                     , LINENAME
                     , SEQ
                     , 누가거리
                     , 지반고
                     , 관저고
                     , 관경
                     , 맨홀
                     , TEXT1
                     , TEXT2
                     , 구간
                     , 구배
                     , INV
                     , SIZE
                     , 라인명
                     , 지하수위
                     , 맨홀_INVERT
                      )        
SELECT @P_PROJECT_CD
      ,@P_LINENAME
	,@P_SEQ
	,@P_누가거리
	,@P_지반고
	,@P_관저고
	,@P_관경
	,@P_맨홀
	,@P_TEXT1
	,@P_TEXT2
	,@P_구간
	,@P_구배	
	,@P_INV
	,@P_SIZE
	,@P_라인명
	,@P_지하수위
	,@P_맨홀_INVERT;</value>
  </data>
  <data name="customSqlQuery2.Sql" xml:space="preserve">
    <value>INSERT INTO PIPE02M00 (  PROJECT_CD	
						,LINENAME
